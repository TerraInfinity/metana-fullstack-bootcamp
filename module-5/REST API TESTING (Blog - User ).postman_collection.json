			"name": "User Tests",
			"item": [
				{
					"name": "Register User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Save email to environment variable\r",
									"pm.test(\"Save email to environment\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    if (jsonData && jsonData.email) {\r",
									"        pm.environment.set(\"loggedInEmail\", jsonData.email);\r",
									"        console.log(\"Saved email: \" + jsonData.email);\r",
									"        if (jsonData && jsonData._id) {\r",
									"            pm.environment.set(\"loggedInUserID\", jsonData._id)\r",
									"        } \r",
									"        if (jsonData && jsonData._id && jsonData.email && jsonData.isAdmin === false) {\r",
									"            pm.environment.set(\"lastNonAdminUserID\", jsonData._id);\r",
									"            pm.environment.set(\"lastNonAdminUserEmail\", jsonData.email);\r",
									"            console.log(\"Saved last non-admin user ID: \" + jsonData._id);\r",
									"            console.log(\"Saved last non-admin user email: \" + jsonData.email);\r",
									"        } else {\r",
									"            console.log(\"User is admin or required data not found. Not updating last non-admin user variables.\");\r",
									"        }\r",
									"    } else {\r",
									"        console.error(\"Email not found in response\");\r",
									"    }\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
